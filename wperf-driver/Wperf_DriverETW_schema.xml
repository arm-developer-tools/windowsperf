<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<instrumentationManifest
    xmlns="http://schemas.microsoft.com/win/2004/08/events"
    xmlns:win="http://manifests.microsoft.com/win/2004/08/windows/events"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.microsoft.com/win/2004/08/events eventman.xsd"
    >
  <instrumentation>
    <events>
      <provider
          guid="{9b15b4b5-6979-4ba7-9b26-00c630a4d7b3}"
          messageFileName="%SystemDrive%\windows\system32\drivers\wperf-driver.sys"
          name="Wperf_Driver"
          resourceFileName="%SystemDrive%\windows\system32\drivers\wperf-driver.sys"
          symbol="DriverControlGuid"
          >
        <channels>
          <channel
            chid="WPERFDRIVER"
            name="Wperf_Driver/Analytic"
            type="Analytic"
            enabled="true"
          />
        </channels>
        <templates>
          <template tid="tid_trace_template">
            <data
              inType="win:AnsiString"
              name="Text"
              outType="xs:Utf8"
            />
            <data
              inType="win:UInt32"
              name="Status"
              outType="xs:unsignedInt"
            />
          </template>
        </templates>
        <events>
          <event
            channel="WPERFDRIVER"
            level="win:LogAlways"
            message="$(string.EventTraceInfo.EventMessage)"
            opcode="win:Info"
            symbol="CustomEventTraceInfo"
            template="tid_trace_template"
            value="5"
          />
        </events>
      </provider>
    </events>
  </instrumentation>
  <localization xmlns="http://schemas.microsoft.com/win/2004/08/events">
    <resources culture="en-US">
      <stringTable>
        <string
          id="EventTraceInfo.EventMessage"
          value="EventTraceInfo"
        />
      </stringTable>
    </resources>
    </localization>
</instrumentationManifest>